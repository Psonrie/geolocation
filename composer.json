{
  "name": "psonrie/geolocation",
  "description": "Laravel service provider to retrieve a users location by their IP Address",
  "authors": [
    {
      "name": "Ger Toska",
      "email": "gcano@psonrie.com"
    }
  ],
  "license": "MIT",
  "keywords": [
    "laravel",
    "FreeGeoIp",
    "geo-location",
    "php",
    "location",
    "geo",
    "geoIP",
    "ip"
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-json": "*",
    "ext-curl": "*",
    "illuminate/support": "~5.0|~6.0|~7.0"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "orchestra/testbench": "~3.2|~4.0",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^0.12.40",
    "vimeo/psalm": "^3.14"
  },
  "archive": {
    "exclude": [
      "/tests"
    ]
  },
  "autoload": {
    "psr-4": {
      "Psonrie\\GeoLocation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Psonrie\\GeoLocation\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "inspect": "phpstan analyse src -l7 --ansi",
    "psalm": "psalm"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Psonrie\\GeoLocation\\GeoLocationServiceProvider"
      ],
      "aliases": {
        "GeoLocation": "Psonrie\\GeoLocation\\Facades\\GeoLocation"
      }
    }
  }
}
